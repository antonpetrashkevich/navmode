{
  "name": "navmode",
  "displayName": "navmode",
  "description": "Turn navmode and commode on/off. So you can use it in your keybindings.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonpetrashkevich/navmode"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "navmode.on",
        "title": "Turn on navmode"
      },
      {
        "command": "commode.on",
        "title": "Turn on commode"
      },
      {
        "command": "modes.off",
        "title": "Turn off navmode and commode"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}